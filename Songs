package view;

public class Songs {
	 String song;
	 String artist;
	 String album;
	 String year;
	 
	 //constructor
	public Songs(String song_temp, String artist_temp, String album_temp, String year_temp){
			song = song_temp;
			artist = artist_temp;
			album = album_temp;
			year = year_temp; 
	}
	
	
	//return information about the Song
	public String getSong(){
		return song;
		
	}
	
	public String getArtist(){
		return artist;
		
	}
	
	public String getAlbum(){
		return album;
		
	}
	
	public String getYear(){
		return year;
		
	}
	
	//changes the information about the song
	public void setSong(String song){
		this.song = song;
		
	}
	
	public void setArtist(String artist){
		this.artist = artist;
		
	}
	
	public void setAlbum(String album){
		this.album = album;
		
	}
	
	public void setYear(String year){
		this.year = year;
		
	}
	
}
